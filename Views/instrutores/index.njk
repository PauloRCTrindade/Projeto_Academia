{% extends "layout.njk" %}

{% block content %}
<div class="card table-container">
    <table width="100%">
        <thead>
            <tr>
                <th>Instruor</th>
                <th>Atendimento</th>
            </tr>
        </thead>
        <tbody>
            {% for item in instrutores%}
            <tr>
                <td>{{item.nome}}</td>
                <td>{{item.funcao}}</td>
            </tr>
            {% endfor %}
            
        </tbody>
    </table>

</div>
{% endblock content %}